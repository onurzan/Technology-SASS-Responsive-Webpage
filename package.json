{
  "name": "technology-sass",
  "version": "1.0.0",
  "description": "The main goal of this project is to use HTML5 and SCSS elements, rules and methods by considering the responsive web-page dynamics to create a full responsive website",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onurzan/tech-app-with-scss.git"
  },
  "author": "Halil Bilgil",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/onurzan/technology-sass-website-responsive"
  },
  "homepage": "https://github.com/onurzan/technology-sass-website-responsive",
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0"
  },
  "devDependencies": {
    "sass": "^1.58.0"
  }
}